untracked fun main(): void {
  filename = arguments()[0];
  contents = FileSystem.readTextFile(filename);

  parseResults = SkipParser.parseSource(contents, false);

  if (parseResults.hasError()) {
    // SkipParse throws a nice error message
    try {
      _ = SkipParse.parse(filename, contents);
    } catch {
    | e ->
      print_error(e.getMessage());
      exit(1)
    }
  } else {
    debug_json(parseResults.tree);
  };

  void
}
