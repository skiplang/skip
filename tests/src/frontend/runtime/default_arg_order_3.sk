class A() extends Exception
class B() extends Exception

fun throw_A(): Int {
  throw A()
}
fun throw_B(): Int {
  throw B()
}

fun foo(_a: Int = throw_A(), _b: Int = throw_B()): void {
  void
}

fun main(): void {
  print_raw(
    try {
      _ = foo();
      "FAIL"
    } catch {
    | A _ -> "Pass\n"
    | B _ -> "FAIL"
    },
  )
}
